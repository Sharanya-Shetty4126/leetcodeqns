//LEETCODE
//NUMBER COMPLIMENT
//CODE BELOW
class Solution {
    public int findComplement(int num) {
 int k=num;
int count=0;
while(k>0){
k/=2;
count++;

}
int i=0;
while(i<count){
if((num&(1<<i))!=0){
    num=num^(1<<i);
}
else{
    num=(num)|((1<<i));
}

i++;

}



return num;


        
    }
}
