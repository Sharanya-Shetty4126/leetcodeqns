//leetcode
//subsets
//bit manipualtion
class Solution {
    public List<List<Integer>> subsets(int[] nums) {


int n=nums.length;
int count=(int)Math.pow(2,n);
List<List<Integer>>list=new ArrayList<>();
int k=0;
while(k<count){

List<Integer>l=new ArrayList<>();
for(int i=0;i<n;i++){
if((k&(1<<i))!=0)
{
    l.add(nums[i]);
}
}

list.add(l);
k++;
}





return list;





        
    }
}
