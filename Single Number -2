//leetcode
//bit manipulation
//medium level qn

class Solution {

public int singleNumber(int[] nums) {
int total=0;
int n=nums.length;

for(int i=0;i<32;i++){
    int count=0;
for(int j=0;j<n;j++){
    if((nums[j]&(1<<i))!=0)count++;
}
if(count%3!=0){
total|=(1<<i);

}


}


return total;
        
    }
}
